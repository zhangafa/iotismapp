.cu-form-group .time-content {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	-ms-flex: 1;
	flex: 1;
	padding-right: 33rpx;
	overflow: hidden;
	position: relative;
}
.cu-form-group .time-content::after {
	font-family: cuIcon;
	display: block;
	content: "\E6A3";
	position: absolute;
	font-size: 31rpx;
	color: #8799a3;
	line-height: 84rpx;
	width: 50rpx;
	text-align: center;
	top: 0;
	bottom: 0;
	right: -16rpx;
	margin: auto;
}

